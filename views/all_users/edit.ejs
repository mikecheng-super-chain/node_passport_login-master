<h4><a href="/all_users">View all users</a></h4>
<h2>Edit Users</h2>
<% include ../partials/messages %>
      <form action="/all_users/<%= newUser.id %>?_method=PUT" method="POST">
        <div class="form-group">
          <label for="name">Name</label>
          <input type="name" name="name" class="form-control" placeholder="Enter Name" value="<%= newUser.name %>">
        </div>
        <a href="/all_users/<%= newUser.id %>">Cancel</a>
        <button type="submit" class="btn btn-primary btn-block">
          Update
        </button>
      </form>